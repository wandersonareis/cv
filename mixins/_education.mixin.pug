mixin educationList(educations)
  each education in educations
    .w3-container
        h5.w3-opacity
            b #{education.InstitutionName}
        h6.w3-text-blue-grey
            i.fa.fa-calendar.fa-fw.w3-margin-right.w3-text-indigo
            | #{education.timeline}
        p #{education.type}
        hr